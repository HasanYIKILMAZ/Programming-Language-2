Q1
#include <stdio.h>

enum day {sunday=1,tuesday,wednesday,thursday,friday,saturday};

int main()
{
	enum day d = thursday;
	printf("The day number stored in d is %d",d);
	return 0;
}
ANS 1
OUTPUT= The day number stored in d is 4

Q2
#include <stdio.h>

enum State {WORKING = 0, FAILED,FREEZED};
enum State currState = 2;

enum State FindState() {
	return currState;
}

int main(){
(FindState() == WORKING)? printf("WORKING"): printf("NOT WORKING");
return 0;	 
}
ANS 2
OUTPUT= NOT WORKING

Q3
#include<stdio.h>

main()
{
	enum result { pass,fail};
	enum result s1,s2;
	s1=pass;
	s2=fail;
	printf("%d",s1);
}
ANS 3
OUTPUT= 0

Q4
#include <stdio.h>

enum example {a=1,b,c};
enum example example1 = 2;
enum example answer()
{
	return example1;
}

int main()
{
	(answer()==a)? printf("yes"): printf("no");
	return 0;
}

ANS 4
OUTPUT = no

Q5
#include <stdio.h>
#define MAX 4

enum sanfoundry
{
	a,b=3,c
};

main()
{
	if(MAX!= c)
		printf("hello");
	else
		printf("welcome");
}
ANS 5
OUTPUT= welcome

