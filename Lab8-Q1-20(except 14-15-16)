Q8
#include <stdio.h>

enum sanfoundry
{
	c=0,
	d=10,
	h=20,
	s=3
}a;

int main()
{
	a=c;
	printf("Size of enum variables = %d bytes",sizeof(a));
	return 0;
}
ANS 8
OUTPUT= Size of enum variables = 4 bytes

